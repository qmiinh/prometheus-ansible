global:
  scrape_interval:     10s
  evaluation_interval: 30s

scrape_configs:
  - job_name: 'victoriametrics'
    static_configs:
      - targets: ['localhost:8428']
  - job_name: prometheus
    honor_labels: true
    static_configs:
    - targets: ['localhost:9090']

remote_write:
- url: http://localhost:8428/api/v1/write
  remote_timeout: 30s
  queue_config:
    capacity: 40000
    max_shards: 60
    max_samples_per_send: 20000